script(
id="js-results__template__a", 
type="text/x-handlebars-template").

    <div class='container'>
        
        <div class='row'>
            
            <h2>{{title}}</h2>
            <h3>{{description}}</h3>
        </div>

        <div class='col-xs-6 js-results__nav' data-result-id={{nav.prev.id}}>
            <p class='h-font-serif'>{{nav.prev.country}}</p>
            <p class='h-font-monospace'>{{nav.prev.title}}</p>
        </div>
        <div class='col-xs-6 js-results__nav' data-result-id={{nav.next.id}}>
            <p class='h-font-serif'>{{nav.next.country}}</p>
            <p class='h-font-monospace'>{{nav.next.title}}</p>
        </div>
    </div>